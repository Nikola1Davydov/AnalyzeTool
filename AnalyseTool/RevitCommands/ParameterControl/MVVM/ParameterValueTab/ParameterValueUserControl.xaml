<UserControl
    x:Class="AnalyseTool.RevitCommands.ParameterControl.MVVM.ParameterValueTab.ParameterValueUserControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:AnalyseTool.RevitCommands.ParameterControl.MVVM.ParameterValueTab"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:wv2="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <!--  d:DataContext="{d:DesignInstance Type=local:ParameterValueViewModel}"  -->


    <DockPanel Margin="10">

        <UniformGrid DockPanel.Dock="Top" Rows="1">
            <ComboBox
                Height="Auto"
                Margin="10"
                ItemsSource="{Binding Parameters}"
                SelectedIndex="0"
                SelectedItem="{Binding SelectedParameter}" />
            <Button Margin="10,0" Content="Create Filters" />
            <Button Content="Clear Filters" />
        </UniformGrid>
        <Grid>
            <wv2:WebView2 x:Name="webView" />
        </Grid>


    </DockPanel>
</UserControl>
